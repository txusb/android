<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".SettingPagr.PrivaryPolicy" android:background="#EFE8E1">
    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:layout_marginTop="27dp"
            android:layout_marginLeft="20dp" android:layout_marginRight="20dp" android:layout_marginBottom="10dp"
            android:background="@color/white" android:id="@+id/sc">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical">
            <TextView
                    android:text="@string/Welcome"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView23"
                    android:lineSpacingMultiplier="1.2" android:textSize="14sp" android:layout_margin="10dp"
                    android:lineSpacingExtra="5dp" android:textColor="@android:color/black"/>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>